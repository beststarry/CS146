    "import numpy as np\n",
    "import scipy.stats as sts\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = []\n",
    "for i in range(1000): x.append([sts.binom.rvs(4,i) for i in sts.beta(1, 1).rvs(3)])\n",
    "\n",
    "for i in range(3): \n",
    "    plt.hist(np.array(x)[:,i])\n",
    "    plt.show()\n",
    "\n",
    "# plt.hist(x[:,1])"
